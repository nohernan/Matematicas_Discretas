graph = { "a" : ["b", "c","d"],
          "b" : ["a", "c"],
          "c" : ["a", "b"],
          "d" : ["a"],
          "e" : []
        }


def generate_edges(graph):
    edges = []
    for node in graph:
        for neighbour in graph[node]:
            edges.append((node, neighbour))

    return edges

print(generate_edges(graph))